    "prettier-plugin-packagejson": "^2.2.18",
    "prop-types": "^15.8.1",
    "query-string": "^7.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "replace": "^1.2.2",
    "semver": "^7.6.3",
    "stylelint": "^14.9.1",
    "tailwindcss": "^4.0.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4",
    "yargs-parser": "^21.1.1",
    "zod": "^3.24.2"
  },
  "peerDependencies": {
