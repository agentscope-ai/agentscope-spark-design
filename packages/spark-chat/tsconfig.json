{
  "compilerOptions": {
    "downlevelIteration": true,
    "lib": [
      "es2021",
      "DOM"
    ],
    "resolveJsonModule": true,
    "checkJs": false,
    "strict": false,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "@agentscope-ai/chat": [
        "components"
      ],
      "@agentscope-ai/chat/*": [
        "components/*",
        "*"
      ]
    }
  },
  "include": [
    ".dumirc.ts",
    "**/*"
  ]
}