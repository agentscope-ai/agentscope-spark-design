{
  "name": "@ali/agentscope-ai-chat",
  "version": "1.1.20",
  "description": "a free and open-source chat framework for building excellent LLM-powered chat experiences",
  "license": "Apache-2.0",
  "sideEffects": [
    "*.less",
    "*.css",
    "*.scss"
  ],
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run src:build && npm run docs:build",
    "build:doc-api": "rm -rf node_modules && tnpm i && npm run start",
    "build:publish": "NPM_PUBLISH=1 npm run src:build",
    "build:watch": "father dev",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:preview": "dumi preview",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "llms:build": "LLM=true dumi build",
    "prepare": "husky install && dumi setup",
    "src:build": "father build",
    "start": "npm run dev"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@agentscope-ai/icons": "^1.0.32",
    "@agentscope-ai/icons-override-antd": "^6.0.0",
    "@agentscope-ai/icons-svg-override-antd": "^4.4.2",
    "@ant-design/cssinjs-utils": "^1.1.3",
