    "lodash": "^4.17.21",
    "marked": "^16.2.1",
    "mermaid": "^11.6.0",
    "pretty-ms": "^9.3.0",
    "react-error-boundary": "^6.0.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.6",
    "react-transition-group": "^4.4.5",
    "recorder-core": "^1.3.25011100",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
