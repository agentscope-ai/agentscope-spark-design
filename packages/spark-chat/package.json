    "@copilotkit/runtime": "^1.9",
    "@copilotkit/runtime-client-gql": "^1.9",
    "@rc-component/motion": "^1.1.4",
    "@react-spring/web": "^10.0.2",
    "ahooks": "^3.8.4",
    "antd-style": "^3.7.1",
    "antd-token-previewer": "^2.0.8",
    "copy-to-clipboard": "^3.3.3",
    "cross-fetch": "^4.1.0",
    "dompurify": "^3.2.6",
    "framer-motion": "^12.12.1",
