    "@ali/dumi-plugin-llms": "^1.0.3",
    "@ali/dumi-theme-spark": "^0.0.97",
    "@ant-design/charts": "^2.6.2",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@makotot/ghostui": "^2.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
