{
  "name": "@agentscope-ai/chat",
  "version": "1.1.45",
  "description": "a free and open-source chat framework for building excellent LLM-powered chat experiences",
  "license": "Apache-2.0",
  "sideEffects": [
    "*.less",
    "*.css",
    "*.scss"
  ],
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "agentscope-runtime-webui": "./bin/cli.js"
  },
  "files": [
    "lib",
    "bin",
    "components"
  ],
  "scripts": {
    "build": "npm run src:build && npm run docs:build",
    "build:doc-api": "rm -rf node_modules && tnpm i && npm run start",
    "build:publish": "NPM_PUBLISH=1 npm run build:cli && npm run src:build",
    "build:watch": "father dev",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:preview": "dumi preview",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "llms:build": "LLM=true dumi build",
    "prepare": "dumi setup",
    "src:build": "father build",
    "start": "npm run dev"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@agentscope-ai/design": "^1.0.0",
    "@agentscope-ai/icons": "^1.0.32",
    "@agentscope-ai/icons-override-antd": "^6.0.0",
    "@agentscope-ai/icons-svg-override-antd": "^4.4.2",
    "@ant-design/cssinjs-utils": "^1.1.3",
    "@ant-design/graphs": "^2.1.1",
    "@ant-design/icons": "^5.0.1",
    "@ant-design/x-markdown": "^2.0.0",
    "ahooks": "^3.8.4",
    "antd-style": "^3.7.1",
    "classnames": "^2.5.1",
    "commander": "^12.1.0",
    "copy-to-clipboard": "^3.3.3",
    "dompurify": "^3.2.6",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "mermaid": "^11.6.0",
    "rc-motion": "^2.9.5",
    "rc-util": "^5.29.1",
    "react-error-boundary": "^6.0.0",
    "react-syntax-highlighter": "^15.6.6",
    "react-transition-group": "^4.4.5",
    "use-context-selector": "^2.0.0",
    "use-stick-to-bottom": "^1.1.1",
    "uuid": "^11.0.5",
    "xstate": "^5.19.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@agentscope-ai/design": "^1.0.0",
    "@agentscope-ai/dumi-theme-spark": "0.0.98",
    "@alibot/dumi-plugin-api-parser": "1.0.3",
    "@alibot/dumi-plugin-llms": "1.0.4",
    "@ant-design/charts": "^2.6.2",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@makotot/ghostui": "^2.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/lint": "^4.0.0",
    "@umijs/plugins": "^4.4.11",
    "antd": "^5.18.0",
    "dumi": "^2.4.13",
    "esbuild": "^0.19.12",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "lucide-react": "^0.561.0",
    "openai": "^4.98.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stylelint": "^14.9.1",
    "tailwindcss": "^3.4.17",
    "vaso": "^0.4.0"
  },
  "peerDependencies": {
    "@agentscope-ai/design": "^1.0.0",
    "@ant-design/icons": "^5.0.1",
    "antd": "^5.18.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "authors": []
}
